{
  "name": "autolabjs",
  "version": "1.0.0",
  "description": "Codebase for backward incompatible revision of autolabcli v0.1.1.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm link",
    "posttest": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "nyc --reporter=lcov --reporter=text-lcov -s --all mocha 'test/*.js' 'test/integration/**/*.js' 'test/lib/**/*.js' > /dev/null",
    "cucumber": "cucumber-js 'test/feature/features' -r 'test/feature/steps' > /dev/null"
  },
  "bin": {
    "autolabjs": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AutolabJS/autolabcli.git"
  },
