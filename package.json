  "license": "GPL v3.0",
  "bugs": {
    "url": "https://github.com/AutolabJS/autolabcli/issues"
  },
  "homepage": "https://github.com/AutolabJS/autolabcli#readme",
  "dependencies": {
    "caporal": "^0.9.0",
    "chalk": "^2.3.0",
    "cli-spinner": "^0.2.8",
    "figlet": "^1.2.0",
    "inquirer": "^5.0.0",
    "preferences": "^1.0.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "cucumber": "^4.0.0",
    "mocha": "^4.1.0",
    "mock-stdin": "^0.3.1",
    "nyc": "^11.4.1",
    "sinon": "^4.1.5",
    "sinon-chai": "^2.14.0"
  }
